package heranca;

import entities.Account;
import entities.BusinessAccount;
import entities.SavingAccount;

public class Program {

	public static void main(String[] args) {
		
		Account acc = new Account(1001, "Alex", 1000.0);
		acc.saque(200.0);
		System.out.println(acc.getSaldo());
		
		Account acc1 = new SavingAccount(1002, "Leonardo", 1000.0, 0.01);
		acc1.saque(200.0);
		System.out.println(acc1.getSaldo());
		
		Account acc2 = new BusinessAccount(1003, "Lidy", 5000.0, 500.0);
		acc2.saque(500);
		System.out.println(acc2.getSaldo());
	}
}
