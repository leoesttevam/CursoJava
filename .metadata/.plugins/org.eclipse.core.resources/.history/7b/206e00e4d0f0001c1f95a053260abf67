package entities;

public class Agencia {

	private int number;
	private String holder;
	private double saldo;
	
	public Agencia() {
	
	}

	public Agencia(int number, String holder) {
		this.number = number;
		this.holder = holder;
	}

	public int getNumber() {
		return number;
	}

	public void setNumber(int number) {
		this.number = number;
	}

	public String getHolder() {
		return holder;
	}

	public void setHolder(String holder) {
		this.holder = holder;
	}

	public double getSaldo() {
		return saldo;
	}
	
	public double saldoDps(double deposito) {
		return saldo += deposito;
	}
	
	public double saldoSqe(double saque) {
		return saldo -= saque + 5;
	}
}
