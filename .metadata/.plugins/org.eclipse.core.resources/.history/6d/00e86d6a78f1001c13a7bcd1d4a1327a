package listas;

import java.util.ArrayList;
import java.util.List;

public class Estudo {

	public static void main(String[] args) {
		
		List<String> list = new ArrayList<>();
		
		list.add("Maria");
		list.add("Alex");
		list.add("Bob");
		list.add("Anna");
		list.add(2, "Marcos");
		
		for(String x : list) {
			System.out.println(x);
		}
		
	}
}
